<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lab.dao.NamesDao">

    <select id="selectEnByID" parameterType="Long" resultType="String">
        select en from names where id = #{id}
    </select>

    <select id="selectZhByID" parameterType="Long" resultType="String">
        select zh from names where id = #{id}
    </select>

</mapper>
